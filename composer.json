{
    "name": "carbon/layout",
    "type": "neos-carbon",
    "description": "Fast and easy layout handler for Neos.io",
    "license": "MIT",
    "keywords": [
        "flow",
        "neos",
        "fusion",
        "helper",
        "layout",
        "carbon"
    ],
    "require": {
        "neos/neos": "^5.3 || ^7.0",
        "carbon/eel": "^1.2 || ^2.0"
    },
    "authors": [
        {
            "name": "Jon Uhlmann",
            "email": "hello@uhlmann.pro",
            "homepage": "https://www.uhlmann.pro",
            "role": "Developer"
        }
    ],
    "archive": {
        "exclude": [
            ".editorconfig",
            ".gitattributes",
            ".github",
            ".gitignore",
            "CODE_OF_CONDUCT.md"
        ]
    },
    "autoload": {
        "psr-4": {
            "Carbon\\Layout\\": "Classes"
        }
    },
    "extra": {
        "installer-name": "Carbon.Layout",
        "neos": {
            "package-key": "Carbon.Layout"
        }
    }
}
